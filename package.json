{
    "name": "heypano-wanikani-helper",
    "version": "1.0.1",
    "description": "Pano's JS WaniKani helper",
    "main": "index.js",
    "browserslist": "last 3 versions",
    "config": {
        "server": {
            "port": 3000,
            "port_COMMENT": "This is used by localtunnel and webpack server"
        }
    },
    "scripts": {
        "clean-dist": "rimraf ./docs && mkdir docs",
        "prebuild": "npm-run-all clean-dist test lint",
        "build": "babel-node buildScripts/build.js",
        "distserver": "babel-node buildScripts/distServer.js",
        "start": "npm-run-all --parallel open:src lint:watch",
        "open:src": "babel-node buildScripts/srcServer.js",
        "localtunnel": "lt --port $npm_package_config_server_port --subdomain heypano",
        "localtunnel_help": "echo \"Localtunnel exposes port $npm_package_config_server_port ($$npm_package_config_server_port) at a url (https://localtunnel.github.io/www/)\"",
        "test": "echo \"No tests yet\"",
        "prebananas": "babel-node buildScripts/prebananas.js",
        "bananas": "babel-node buildScripts/bananas.js",
        "postbananas": "babel-node buildScripts/postbananas.js",
        "security_check": "npm audit --audit-level high",
        "help": "babel-node buildScripts/help.js",
        "lint": "esw --color webpack.config.* src buildScripts",
        "lint:watch": "npm run lint -- --watch",
        "generateMockData": "babel-node buildScripts/generateMockData.js",
        "prestart-mockapi": "npm run generateMockData",
        "start-mockapi": "json-server --watch src/api/db.json --port 3001"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^5.8.1",
        "autoprefixer": "^9.6.0",
        "babel-polyfill": "^6.26.0",
        "bootstrap": "^4.3.1",
        "copy-to-clipboard": "^3.0.8",
        "dompurify": "^1.0.8",
        "html-react-parser": "^0.4.7",
        "kuromoji": "^0.1.2",
        "kuroshiro": "^1.1.1",
        "kuroshiro-analyzer-kuromoji": "^1.1.0",
        "wanakana": "^4.0.2",
        "jquery": "^3.4.0",
        "lodash": "^4.17.11",
        "postcss-loader": "^3.0.0",
        "prop-types": "^15.7.2",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-redux": "^7.0.2",
        "react-router-dom": "^5.0.0",
        "reactstrap": "^8.0.0",
        "redux": "^4.0.1",
        "redux-freeze": "^0.1.7",
        "redux-logger": "^3.0.6",
        "redux-thunk": "^2.3.0",
        "whatwg-fetch": "^3.0.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.4.3",
        "@babel/core": "^7.4.3",
        "@babel/node": "^7.2.2",
        "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
        "@babel/preset-env": "^7.4.3",
        "@babel/preset-react": "^7.0.0",
        "@babel/register": "^7.4.0",
        "babel-loader": "^8.0.5",
        "chai": "^4.2.0",
        "chalk": "^2.4.2",
        "cheerio": "^1.0.0-rc.3",
        "compression": "^1.7.4",
        "connect-history-api-fallback": "^1.6.0",
        "copy-webpack-plugin": "^5.0.2",
        "cross-env": "^5.2.0",
        "css-loader": "^2.1.1",
        "eslint": "^5.16.0",
        "eslint-plugin-import": "^2.17.2",
        "eslint-plugin-react": "^7.12.4",
        "eslint-watch": "^5.0.1",
        "express": "^4.17.3",
        "faker": "^4.1.0",
        "file-loader": "^3.0.1",
        "html-webpack-plugin": "^3.2.0",
        "jsdom": "^14.0.0",
        "json-schema-faker": "^0.5.0-rc9",
        "json-server": "^0.14.2",
        "localtunnel": "^1.9.1",
        "mini-css-extract-plugin": "^0.6.0",
        "mocha": "^6.1.4",
        "nock": "^10.0.6",
        "node-sass": "^4.12.0",
        "npm-run-all": "^4.1.5",
        "numeral": "^2.0.6",
        "open": "^6.1.0",
        "rimraf": "^2.6.3",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "webpack": "^4.30.0",
        "webpack-dev-middleware": "^3.6.2",
        "webpack-hot-middleware": "^2.24.3",
        "webpack-md5-hash": "0.0.6"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/heypano/wanikanihelper.git"
    },
    "keywords": [
        "js",
        "starter",
        "kit",
        "npm",
        "babel"
    ],
    "author": "Pano Papadatos (www.heypano.com)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/heypano/wanikanihelper/issues"
    },
    "homepage": "https://github.com/heypano/wanikanihelper#readme"
}
